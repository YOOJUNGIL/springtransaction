<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myalley.springtransaction.dao.service.custuser.CustUserMapper">
	
	<select id="getCustUserList" parameterType="String" resultType="custUser">
		select USER_ID
		     , MOBILE_NO
		     , ADDR
		  from cust_user
	</select>
	
	<insert id="save" parameterType="custUser">
		insert into cust_user 
		values (
			#{userId}
		  , #{mobileNo}
		  , #{addr}
		)
	</insert>
	
</mapper>